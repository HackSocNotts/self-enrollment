{
  "name": "self-enrollment",
  "scripts": {
    "lint": "eslint . --ignore-path .gitignore --ext .js,.ts",
    "lint:staged": "eslint --ignore-path .gitignore",
    "lint:fix": "npm run lint -- --fix",
    "pretest": "npm run lint",
    "build": "tsc",
    "start": "tsc-watch --onSuccess 'node lib/index.js'",
    "test": "jest --watchAll=false --verbose=false",
    "test:watch": "jest --watchAll --verbose=false"
  },
  "engines": {
    "node": "12"
  },
  "main": "lib/index.js",
  "dependencies": {
    "@hapi/joi": "^15.1.0",
    "@types/bunyan": "^1.8.6",
    "@types/node": "^12.6.8",
    "@types/sinon": "^7.0.13",
    "body-parser": "^1.19.0",
    "celebrate": "^10.0.1",
    "cors": "^2.8.5",
    "deep-object-diff": "^1.1.0",
    "express": "^4.17.1",
    "sinon": "^7.3.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/cors": "^2.8.5",
    "@types/express": "^4.17.0",
    "@types/hapi__joi": "^15.0.2",
    "@types/jest": "^24.0.15",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "@typescript-eslint/parser": "^1.11.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-jsdoc": "^15.6.1",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^3.0.0",
    "jest": "^24.8.0",
    "jest-junit": "^6.4.0",
    "lint-staged": "^9.1.0",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "tsc-watch": "^4.2.3",
    "typescript": "^3.2.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "private": true,
  "lint-staged": {
    "*.{js,ts}": [
      "npm run lint:staged"
    ]
  }
}
